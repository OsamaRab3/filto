{
  "name": "filto",
  "description": "A fast, lightweight, and developer-friendly CLI tool for converting files with a single command",
  "version": "1.0.1",
  "author": {
    "name": "OsamaRab3",
    "email": "osama.rab3a@gmail.com",
    "url": "https://github.com/OsamaRab3"
  },
  "bin": {
    "filto": "bin/run.js"
  },
  "bugs": "https://github.com/OsamaRab3/filto/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "asposepdfnodejs": "^25.10.0",
    "image-to-pdf": "^3.0.2",
    "rewiremock": "^3.14.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/pdfkit": "^0.17.3",
    "@types/sinon": "^21.0.0",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^21.0.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./src",
    "README.md",
    "LiCENSE",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/OsamaRab3/filto",
  "keywords": [
    "cli",
    "file-converter",
    "pdf",
    "image",
    "document",
    "conversion"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "filto",
    "dirname": "filto",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "convert": {
        "description": "Convert files between different formats"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OsamaRab3/filto.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
